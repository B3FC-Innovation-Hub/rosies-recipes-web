{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.{jpg,gif,png,gif,png,svg,css}",
            "assets/*"
        ]
    },
    "trailingSlash": "auto",
    "routes": [
        {
            "route": "/",
            "rewrite": "/index.html"
        },
        {
            "route": "/home*",
            "allowedRoles": ["authenticated"]
        }
    ]
}