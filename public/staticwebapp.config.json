{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/static/media/*.{png,jpg,jpeg,gif,bmp}",
            "/static/css/*",
            "assets/*"
        ]
    },
    "routes": [
        {
            "route": "/"
        },
        {
            "route": "/home*",
            "rewrite": "/home.html"
        },
        {
            "route": "/404",
            "rewrite": "/404.html"
        }
    ],
    "globalHeaders": {
        "Access-Control-Allow-Origin": "https://lemon-wave-01bf94003.3.azurestaticapps.net",
        "Access-Control-Allow-Methods": "POST, GET, OPTIONS"
    }
}
